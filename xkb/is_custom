    include "us"

    key <AB01>	{ [ dead_acute, dead_acute, z, Z ] };

    key <AB10>	{ [ thorn, THORN, slash, question ] };

    key <AC01>	{ [ a, A, ae, AE ] };

    key <AC03>	{ [ d, D, eth, ETH ] };

    key <AC11>	{ [ dead_acute, dead_acute, dead_circumflex, dead_caron ] }; // ' -> Â´

    key <AD09>	{ [ o, O, odiaeresis, Odiaeresis ]	};

    include "level3(ralt_switch)"
};
